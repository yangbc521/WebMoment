<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>UserManager</servlet-name>
        <servlet-class>cn.xuhuanfeng.webmoment.controller.UserManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EssayManager</servlet-name>
        <servlet-class>cn.xuhuanfeng.webmoment.controller.EssayManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>cn.xuhuanfeng.webmoment.controller.User</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserManager</servlet-name>
        <url-pattern>/UserManager/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EssayManager</servlet-name>
        <url-pattern>/EssayManager/*</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
